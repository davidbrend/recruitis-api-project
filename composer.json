{
    "name": "davebrend/recruitis-api-project",
    "autoload": {
        "psr-4": {
            "Davebrend\\RecruitisApiProject\\": "src/"
        }
    },
    "type": "library",
    "description": "Recruitis API package",
    "keywords": ["recruitis","api"],
    "license": "MIT",
    "authors": [
        {
            "name": "David Brend",
            "email": "brend@universe.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^5.0 || ^6.0",
        "guzzlehttp/guzzle": "*",
        "nesbot/carbon": "^3.5",
        "symfony/yaml": "^7.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/test-pack": "^1.1",
        "phpunit/phpunit": "^9.6",
        "phpstan/phpstan": "^1.11"
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse src --level 8",
        "run-tests": "vendor/bin/phpunit"
    }
}
